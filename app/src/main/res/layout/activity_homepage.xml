        android:layout_alignParentTop="true"
        android:background="#ffffff"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-1dp" />

    <View
        android:id="@+id/bar"
        android:layout_width="411dp"
        android:layout_height="3dp"
        android:layout_alignParentTop="true"
